<a href="/add_pomodoro">Add Pomarolos</a>
<br><br>
<table>
  <tr>
    <th>Description</th>
    <th>Pomarolos</th>
    <th>Interruption</th>
    <th>Finish</th>
  </tr>
  % pomodoros.each do |pomodoro|
  <tr>
    <td>{{ pomodoro.description }}</td>
    <td>{{ pomodoro.pomarolo }}</td>
    <td>{{ pomodoro.interruption }}</td>
    <td><a href="/pomarolo/{{ pomodoro.id }}">{{ pomodoro.finish }}</a></td>
  </tr>
  % end
</table>
