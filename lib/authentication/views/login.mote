<h3>Login</h3>
<form action="/login" method="POST">
  <label>Email</label>
  <input type="text" name="user[email]" value="{{ params["email"] }}">
  <br>

  <label>Password</label>
  <input type="password" name="user[password]" value="">

  <div>
    <button type="submit" name="login">Login</button>
  </div>
</form>
